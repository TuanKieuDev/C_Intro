#include <stdio.h>

int main()
{
  int choice;
  do{
    printf("Menu\n");
    printf("1. Lab 1 \n");
    printf("2. Lab 2 \n");
    printf("Enter 0 to quit");
    printf("Enter your choice: ");
    scanf("%d",&choice);
    switch(choice){
    case 1:
      printf("Do lab 1");
      break;
    case 2:
      printf("Do lab 2");
      break;
    default:
      printf("Not valid");
    }
  }while(choice!=0);
    return 0;
}
